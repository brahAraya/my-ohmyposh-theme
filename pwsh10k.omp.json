{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"segments": [
				{
					"foreground": "#ffffff",
					"style": "plain",
					"template": "\n{{ .UserName }} \u279c ",
					"type": "session"
				},
				{
					"foreground": "#7E46B6",
					"properties": {
						"folder_icon": "\uf07b",
						"folder_separator_icon": " / ",
						"home_icon": "\uf7db",
						"style": "agnoster"
					},
					"style": "plain",
					"template": "{{ .Path }} ",
					"type": "path"
				},
				{
					"foreground": "#5FD7FF",
					"properties": {
						"branch_ahead_icon": "",
						"branch_behind_icon": "",
						"branch_gone_icon": "",
						"branch_icon": "\ue725 ",
						"branch_identical_icon": "",
						"cherry_pick_icon": "",
						"commit_icon": "",
						"merge_icon": "",
						"rebase_icon": "",
						"revert_icon": "",
						"tag_icon": "",
						"fetch_stash_count": true,
						"fetch_status": true,
						"fetch_upstream_icon": true,
						"fetch_worktree_count": true
					},
					"style": "plain",
					"template": "<#ffffff>(</>{{ .HEAD }}{{ if .Staging.Changed }}<#87FF00> \u25cf {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#D75F00> \u25cf {{ .Working.String }}</>{{ end }}<#ffffff>)</> ",
					"type": "git"
				},
				{
					"foreground": "#ffe644",
					"properties": {
						"fetch_version": true,
						"fetch_package_manager": true,
						"npm_icon": "<#cc3a3a>(\ue5fa)</>",
						"yarn_icon": " <#348cba>(\uf61a)</>"
					},
					"style": "plain",
					"template": "<#ffffff>- [</>\ue781 v{{ .Full }}<#ffffff>]</>",
					"type": "node"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "right",
			"segments": [
				{
					"foreground": "#ffffff",
					"properties": {
						"always_enabled": true
					},
					"style": "plain",
					"template": "\uf608 {{ .FormattedMs }} | ",
					"type": "executiontime"
				},
				{
					"foreground": "#7E46B6",
					"properties": {
						"time_format": "15:04:05"
					},
					"style": "plain",
					"template": "{{ .CurrentDate | date .Format }}",
					"type": "time"
				},
				{
					"foreground": "#ffffff",
					"properties": {
						"always_enabled": true
					},
					"style": "plain",
					"template": " {{ if gt .Code 0 }}<red>\uf659</>{{ else }}<#87FF00>\uf633</>{{ end }} \uf489",
					"type": "exit"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "#87FF00",
					"style": "plain",
					"template": "❯ ",
					"type": "text"
				}
			],
			"type": "prompt"
		}
	],
	"version": 2
}